{
	"name": "goetas-webservices/xsd2php",
	"description": "Convert XSD (XML Schema) definitions into PHP classes and JMS metadata",
	"type": "library",
	"authors": [
		{
			"name": "Asmir Mustafic"
		}
	],
	"keywords": [
		"converter",
		"xml",
		"xsd",
		"php",
		"jms",
		"serializer"
	],
	"license": "MIT",
	"require": {
		"php": ">=5.6",
		"symfony/console": "^2.7|^3.0",
		"symfony/dependency-injection": "^2.2|^3.0",
		"symfony/config": "^2.2|^3.0",
		"symfony/yaml": "^2.1|^3.0",
		"doctrine/inflector": "^1.0",
		"zendframework/zend-code": "~2.3",

		"psr/log": "^1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^4.8|^5.0",
		"jms/serializer": "^1.3",
		"goetas-webservices/xsd2php-runtime": "^0.2.7",
		"goetas-webservices/xsd-reader": "dev-static-analysis",
		"goetas-webservices/soap-reader": "dev-static-analysis-integration",
		"goetas-webservices/wsdl-reader": "dev-static-analysis-integration"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/SignpostMarv/xsd-reader.git"
		},
		{
			"type": "vcs",
			"url": "https://github.com/SignpostMarv/soap-reader.git"
		},
		{
			"type": "vcs",
			"url": "https://github.com/SignpostMarv/wsdl-reader.git"
		}
	],
	"autoload": {
		"psr-4": {
			"GoetasWebservices\\Xsd\\XsdToPhp\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"GoetasWebservices\\Xsd\\XsdToPhp\\Tests\\": "tests/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.2-dev"
		}
	},
	"bin": [
		"bin/xsd2php"
	]
}
